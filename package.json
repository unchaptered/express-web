{
  "name": "express-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prestart": "npm run build",
    "build": "babel src/app.js",
    "start": "cross-env NODE_ENV=prod babel-node ./dist/app.js",
    "start:dev": "cross-env NODE_ENV=dev nodemon --exec babel-node ./src/app.js",
    "start:test": "cross-env NODE_ENV=test babel-node ./src/app.js",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:vs": "jest --verbose",
    "test:cov": "jest --coverage",
    "test:cov:vs": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:watch:vs": "jest --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unchaptered/express-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unchaptered/express-web/issues"
  },
  "homepage": "https://github.com/unchaptered/express-web#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
